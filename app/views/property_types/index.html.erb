<h1>Listing property_types</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @property_types.each do |property_type| %>
      <tr>
        <td><%= property_type.description %></td>
        <td><%= link_to 'Show', property_type %></td>
        <td><%= link_to 'Edit', edit_property_type_path(property_type) %></td>
        <td><%= link_to 'Destroy', property_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property type', new_property_type_path %>
